scope main {

    norm n : started & play(U, unit, _)
        -> obligation(U, n, vl(X)[source(U)] & X>5, deadline)
    .

    norm des1 : design(Who, n, modify(vl))
        -> obligation(Who, des1, designed(modify, n, new_object(vl(X))), `2 minutes`)
    .

    norm des2 : design(Who, n, reassign(vl))
        -> obligation(Who, des2, designed(modify, n, new_subject(U)), `2 minutes`)
    .

    norm exe : execute(Who, What, des(OP,N1,N2))
        -> obligation(Who, exe, executed(des(OP,N1,N2)), `2 minutes`)
    .
}