scope main {

    norm n : order(N)[source(order)] & play(U, unit, _)
        -> obligation(U, n, vl(N, X)[source(U)] & X>5, deadlineOrder(N))
    .

    norm des1 : design(Who, n, modify(vl))
        -> obligation(Who, des1, designed(modify(subject), n, new(Cond, Cons)), `2 minutes`)
    .

    norm des2 : design(Who, n, reassign(vl))
        -> obligation(Who, des2, designed(modify(object), n, new(Cond, Cons)), `2 minutes`)
    .

    norm exe : execute(Who, N1, des(OP, new(Cond, Cons)))
        -> obligation(Who, exe, executed(N1, des(OP, new(Cond, Cons))), `2 minutes`)
    .
}